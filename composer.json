{
    "name": "krzar/laravel-translation-generator",
    "description": "Translations generator for laravel apps.",
    "keywords": [
        "laravel",
        "translations",
        "translations-generator"
    ],
    "license": "MIT",
    "version": "v4.0.0",
    "authors": [
        {
            "name": "Krystian ZarÄ™ba"
        }
    ],
    "autoload": {
        "psr-4": {
            "Krzar\\LaravelTranslationGenerator\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Krzar\\LaravelTranslationGenerator\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": "^8.2",
        "illuminate/console": "^11.0|^12.0",
        "illuminate/support": "^11.0|^12.0",
        "illuminate/collections": "^11.0|^12.0",
        "illuminate/container": "^11.0|^12.0",
        "laravel/prompts": "^0.1.11|^12.0",
        "ext-json": "*"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Krzar\\LaravelTranslationGenerator\\LaravelTranslationGeneratorServiceProvider"
            ]
        }
    },
    "require-dev": {
        "laravel/pint": "^1.4",
        "phpunit/phpunit": "^11.0",
        "mockery/mockery": "^1.6"
    }
}
